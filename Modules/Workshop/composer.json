{
  "name": "idavoll/workshop-module",
  "type": "lioncore-module",
  "description": "Workshop module for lioncore. Handles the activation of modules.",
  "keywords": [
    "lioncore",
    "workshop",
    "modules"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@lioncore.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@lioncore.com",
    "issues": "https://github.com/lioncore/Platform/issues",
    "source": "https://github.com/lioncore/Workshop"
  },
  "require": {
    "php": ">=7.0.0",
    "composer/installers": "~1.0",
    "symfony/yaml": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "3.5.*",
    "spatie/phpunit-snapshot-assertions": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Workshop\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
