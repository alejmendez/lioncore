{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "enlightn/enlightn": "^2.1.0",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.4.5",
        "laravel/framework": "^9.22.1",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7.2",
        "musonza/chat": "^6.0.0",
        "owen-it/laravel-auditing": "^13.0.4",
        "spatie/laravel-permission": "^5.5.5",
        "tucker-eric/eloquentfilter": "^3.1.0",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20.0",
        "laravel/sail": "^1.15.1",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.2.1",
        "phpunit/phpunit": "^9.5.21",
        "stichoza/google-translate-php": "^4.1.6",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Alejmendez87\\Workshop\\": "Packages/Alejmendez87/Workshop/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
